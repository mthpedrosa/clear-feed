{
    "manifest_version": 3,
    "name": "ClearFeed: Blocker de Reels e Shorts",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "content_scripts": [
      {
        "matches": [
          "*://*.youtube.com/*",
          "*://*.facebook.com/*",
          "*://*.instagram.com/*"
        ],
        "js": ["content.js"]
      }
    ]
  }